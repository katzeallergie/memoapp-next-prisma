{
  "name": "memoapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema ./prisma/schema.prisma && prisma db push && next build",
    "build:prod": "prisma generate --schema ./prisma/dev/schema.prisma && prisma db push &&  next build",
    "start": "next start",
    "local": "NODE_ENV=local next dev",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "@prisma/client": "^5.5.2",
    "@vercel/postgres": "^0.5.1",
    "dotenv-cli": "^7.3.0",
    "framer-motion": "^10.16.4",
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "20.9.4",
    "@types/react": "18.2.38",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "prisma": "^5.5.2",
    "tailwindcss": "^3.3.0",
    "typescript": "5.3.2"
  }
}
